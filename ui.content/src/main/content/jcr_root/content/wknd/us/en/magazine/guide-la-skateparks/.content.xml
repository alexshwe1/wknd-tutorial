                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wknd/components/text"
                        text="&lt;blockquote>“There is no better place to shred than Los Angeles.”&lt;/blockquote>&#xd;&#xa;&lt;p>&amp;nbsp;- Jacob Wester, Pro Skater&lt;/p>&#xd;&#xa;"
